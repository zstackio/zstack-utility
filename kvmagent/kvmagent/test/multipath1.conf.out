defaults {
    find_multipaths yes
    user_friendly_names yes
}
devices {
device {
    detect_path_checker yes
    detect_prio yes
    failback immediate
    hardware_handler 1 emc
    no_path_retry 60
    path_checker emc_clariion
    path_grouping_policy group_by_prio
    prio emc
    product .*
    product_blacklist LUNZ
    retain_attached_hw_handler yes
    rr_weight uniform
    vendor DGC
}
device {
    features 0
    no_path_retry fail
    product *
    vendor *
}

}
blacklist {
    wwid *
}